{
    "name": "industrialdev/wicket-wp-account-centre",
    "description": "Wicket's Account Centre for WordPress",
    "version": "1.5.3",
    "keywords": [
        "wordpress",
        "wicket",
        "account",
        "centre"
    ],
    "license": "GPLv2 or later",
    "homepage": "https://wicket.io",
    "authors": [
        {
            "name": "Wicket Inc.",
            "homepage": "https://wicket.io"
        }
    ],
    "type": "wordpress-plugin",
    "require": {
        "php": ">=8.1.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.64"
    },
    "scripts": {
        "post-install-cmd": [
            "chmod +x .ci/install-hooks.sh",
            "bash .ci/install-hooks.sh",
            "chmod +x .git/hooks/pre-commit",
            "if [ \"$COMPOSER_DEV_MODE\" = \"0\" ]; then rm -rf ./.ci; fi"
        ],
        "post-update-cmd": [
            "chmod +x .ci/install-hooks.sh",
            "bash .ci/install-hooks.sh",
            "chmod +x .git/hooks/pre-commit",
            "if [ \"$COMPOSER_DEV_MODE\" = \"0\" ]; then rm -rf ./.ci; fi"
        ]
    }
}
